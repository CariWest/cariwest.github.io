<div class="container-fluid room-for-navbar">

  <h1>Sign Up</h1>

  <%= form_for :user do |f| %>
    <p>Username: <%= f.text_field :username %></p>
    <p>Email: <%= f.text_field :email %></p>
    <p>Password: <%= f.password_field :password_digest %></p>
    <p>Password: <%= f.password_field :password_confirmation %></p>
    <p><%= f.submit %></p>

  <% end %>

</div>
